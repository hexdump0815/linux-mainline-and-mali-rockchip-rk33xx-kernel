&i2s0 {
	status = "okay";

	i2s0_p0: port {
		i2s0_p0_0: endpoint {
			dai-format = "i2s";
			mclk-fs = <256>;
			remote-endpoint = <&hdmi_p0_0>;
		};
	};
};

&i2s1 {
	status = "okay";

	i2s1_p0: port {
		i2s1_p0_0: endpoint {
			dai-format = "i2s";
			mclk-fs = <256>;
			remote-endpoint = <&codec_p0_0>;
		};
	};
};

&io_domains {
	status = "okay";

