/dts-v1/;
#include "rk3328.dtsi"

/ {
	model = "MX10";
	compatible = "mx10,mx10", "rockchip,rk3328";

	chosen {
		bootargs = "earlycon=uart8250,mmio32,0xff130000";
		stdout-path = "serial2:1500000n8";
	};

